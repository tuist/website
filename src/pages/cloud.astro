---
import BaseLayout from "../layouts/BaseLayout.astro";
import metadata from '../utils/metadata.json'
import { AstroSeo } from "@astrolib/seo";
import PricingThree from "../components/pricing/PricingThree.astro";
import LogoCloudOne from "../components/testimonials/LogoCloudOne.astro";
import FaqThree from "../components/Infopages/FaqThree.astro";
import CtaFour from "../components/ctas/CtaFour.astro";
import FeatureOne from "../components/features/FeatureOne.astro";

---

<AstroSeo
  title="Tuist Cloud - A premium extension for Tuist users"
  description=`Discover Tuist Cloud: a premium extension for Tuist users. Boost productivity, enhance collaboration, make informed decisions, and ensure your project's health. Exclusive features tailored for those who demand excellence.`
  canonical={metadata.url}
  openGraph={{
    url: metadata.url,
    title: "Tuist Cloud - A premium extension for Tuist users",
    description: `Discover Tuist Cloud: a premium extension for Tuist users. Boost productivity, enhance collaboration, make informed decisions, and ensure your project's health. Exclusive features tailored for those who demand excellence.`,
    images: [
      {
        url: `${metadata.url}/cloud/og-image.jpg`,
        alt: "Tuist Cloud",
        type: "image/jpg",
        height: 630,
        width: 1200
      },
    ],
    site_name: "Tuist",
  }}
  twitter={{
    handle: "@tuistio",
    site: "@tuistio",
    cardType: "summary_large_image",
  }}
/>
<BaseLayout>
    <FeatureOne/>
    <PricingThree />
    <!-- <LogoCloudOne /> -->
    <FaqThree />
    <CtaFour />
</BaseLayout>
