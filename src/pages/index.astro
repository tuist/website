---
import BaseLayout from "../layouts/BaseLayout.astro";

import LogocCloudOne from "../components/testimonials/LogoCloudOne.astro";
import FeatureFour from "../components/features/FeatureFour.astro";
import FeatureTwo from "../components/features/FeatureTwo.astro";
import FeatureThree from "../components/features/FeatureThree.astro";
import TestimonialOne from "../components/testimonials/TestimonialOne.astro";
import Faq from "../components/Infopages/Faq.astro";
import CtaThree from "../components/ctas/CtaThree.astro";
import metadata from '../content/metadata.js';
import { AstroSeo } from "@astrolib/seo";
---

<AstroSeo
  title={`Tuist - ${metadata.description}`}
  description={metadata.description}
  canonical="https://tuist.io"
  openGraph={{
    url: "https://tuist.io",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://tuist.io/images/logo.jpeg",
        alt: "Tuist logo",
        type: "image/jpeg",
      }
    ],
    site_name: "Tuist",
  }}
  twitter={{
    handle: "@tuistio",
    site: "@site",
    cardType: "summary",
  }}
/>
<BaseLayout>
  <!-- <HeroTwo /> -->
  <FeatureTwo />
  <LogocCloudOne />
  <FeatureFour />
  <TestimonialOne />


  <!-- <FeatureOne /> -->
  <FeatureThree />
  <!-- <Pricing /> -->
  <Faq />
  <CtaThree />

  <!-- <Cta /> -->
</BaseLayout>
