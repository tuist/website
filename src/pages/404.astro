---
import SplashScreenLayout from "../layouts/SplashScreenLayout.astro";
import Error404 from "../components/Infopages/Error404.astro";
import { AstroSeo } from "@astrolib/seo";
---

<AstroSeo
  title="404"
  description="Not found page"
  canonical="https://tuist.io"
  openGraph={{
    url: "https://tuist.io",
    title: "Open Graph Title",
    description: "Open Graph Description",
    images: [
      {
        url: "https://tuist.io/images/logo.jpeg",
        alt: "Tuist logo",
        type: "image/jpeg",
      }
    ],
    site_name: "Tuist",
  }}
  twitter={{
    handle: "@tuistio",
    site: "@site",
    cardType: "summary",
  }}
/>
<SplashScreenLayout>
  <Error404 />
</SplashScreenLayout>
