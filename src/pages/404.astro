---
import SplashScreenLayout from "../layouts/SplashScreenLayout.astro";
import Error404 from "../components/Infopages/Error404.astro";
import { AstroSeo } from "@astrolib/seo";
import metadata from '../content/metadata.js'
---

<AstroSeo
  title="404"
  description="Not found page"
  canonical={metadata.url}
  openGraph={{
    url: metadata.url,
    title: `404 - Tuist`,
    description: "Not found page",
    images: [
      {
        url: `${metadata.url}/images/logo.jpeg`,
        alt: "Tuist logo",
        type: "image/jpeg",
      }
    ],
    site_name: "Tuist",
  }}
  twitter={{
    handle: "@tuistio",
    site: "@tuistio",
    cardType: "summary",
  }}
/>
<SplashScreenLayout>
  <Error404 />
</SplashScreenLayout>
